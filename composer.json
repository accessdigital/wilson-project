{
    "name": "accessdigital/wilson-project",
    "description": "Project template for a Wilson codebase. This is the Wilson equivalent to running 'composer create-project drupal/recommended-project'.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
      "composer/installers": "^2.0",
      "cweagans/composer-patches": "^1.7",
      "drupal/core-composer-scaffold": "^10.0",
      "drupal/core-project-message": "^10.0",
      "drupal/core-recommended": "^10.0",
      "drupal/admin_login_path": "^2.0",
      "drupal/admin_toolbar": "^3.3",
      "drupal/block_field": "^1.0@RC",
      "drupal/color_field": "^3.0",
      "drupal/config_split": "^2.0@RC",
      "drupal/cookies": "^1.2",
      "drupal/default_content": "^2.0@alpha",
      "drupal/devel": "^5.0",
      "drupal/diff": "^1.1",
      "drush/drush": "^11.1",
      "drupal/editor_advanced_link": "^2.1",
      "drupal/environment_indicator": "^4.0",
      "drupal/field_group": "^3.4",
      "drupal/focal_point": "^2.0@alpha",
      "drupal/google_tag": "^1.6",
      "drupal/image_effects": "^3.4",
      "drupal/imagemagick": "^3.4",
      "drupal/mail_login": "^2.6",
      "drupal/material_icons": "^2.0",
      "drupal/menu_block": "^1.6",
      "drupal/menu_link_attributes": "^1.3",
      "drupal/metatag": "^1.22",
      "drupal/paragraphs": "^1.15",
      "drupal/paragraphs_ee": "^2.1@alpha",
      "drupal/password_policy": "^4.0",
      "drupal/pathauto": "^1.11",
      "drupal/rabbit_hole": "^1.0@beta",
      "drupal/redirect": "^1.8",
      "drupal/seckit": "^2.0",
      "drupal/shield": "^1.7",
      "drupal/simple_gmap": "3.1.x-dev@dev",
      "drupal/simple_sitemap": "^4.1",
      "drupal/twig_field_value": "^2.0",
      "drupal/twig_tweak": "^3.2",
      "drupal/username_enumeration_prevention": "^1.3",
      "drupal/viewsreference": "^2.0@beta",
      "drupal/webform": "^6.2@beta",
      "drupal/webp": "^1.0@beta"
    },
    "require-dev": {
        "drupal/core-dev": "^10.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "patches": {
            "drupal/core": {
              "3267870: Order image mappings by breakpoint ID and numeric multiplier": "https://www.drupal.org/files/issues/2022-05-19/3267870-9.5.x-47.patch",
              "3192234: Apply width and height attributes to responsive image tag": "https://www.drupal.org/files/issues/2023-04-07/3192234-293-10.0.x.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-core-project-message": {
            "include-keys": ["homepage", "support"],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                          </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Wilson template!  </>",
                "<bg=blue;fg=white>                                                          </>",
                ""
            ]
        }
    }
}
